{
  "version": 2,
  "builds": [
    { 
      "src": "main.py", 
      "use": "@vercel/python",
      "config":{
        "runtime": "python3.12"
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "main.py" }
  ], 
  "excludeFiles": [
    "__pycache__/**",
    "*.pyc",
    ".pytest_cache/**",
    "*.egg-info/**",
    ".env",
    ".venv/**"
  ]
}
